    function hacka.dickfack(b,c,d)
      local e=b:CommandNumber()
      local f=a.Psm(e)
      local g=x[f][1]
      local h=x[f][2]
      local i=c:Forward()
      local j=c:Right()
      local k=c:Up()
      return i+g*d.x*j*-1+h*d.y*k*-1
    end
    
